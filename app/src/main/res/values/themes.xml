<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.OnboardingJetpackCompose" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:statusBarColor">#FA0101</item>
    </style>
    
    <style name="Theme.SplashScreenFirst" parent="Theme.SplashScreen" tools:ignore="ResourceCycle">
        <item name="windowSplashScreenBackground">@color/white</item>
        <item name="windowSplashScreenAnimationDuration">600</item>
        <item name="postSplashScreenTheme">@style/Theme.OnboardingJetpackCompose</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/splashbloodred</item>
    </style>
</resources>